{
  "private": true,
  "name": "mpa-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for multi-page application",
  "scripts": {
    "exec": "gulp --cwd=./ --gulpfile scripts/gulpfile.js",
    "clean": "npm run exec clean",
    "vendor": "npm run exec vendor",
    "symbols:gen": "npm run exec symbols:gen",
    "iconfont:gen": "npm run exec iconfont:gen",
    "build": "cross-env NODE_ENV=production npm run exec build",
    "watch": "cross-env-shell NODE_ENV=development \"npm run clean && npm run vendor && npm run exec watch\"",
    "dev": "cross-env-shell NODE_ENV=development \"npm run clean && npm run vendor && npm run exec server\"",
    "lint-scripts": "eslint --fix --ext .js scripts/",
    "lint": "eslint --fix --ext .js,.vue assets/"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "assets/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evan2x/mpa-boilerplate.git"
  },
  "author": {
    "name": "evan2x",
    "email": "evan2zaw@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/evan2x/mpa-boilerplate/issues"
  },
  "homepage": "https://github.com/evan2x/mpa-boilerplate#readme",
